"3.12.0 (created: 2016/02/28 22:07:56)

highlight CmdLine background: white; color: black; transition: all 0.25s; font-size:110%; 
highlight Hint font-size:140%;color:white;background-color:blue;padding:2px;
set complete=l
set focuscontent
set hintchars=hjklasdfgyuiopqwertnmzxcvb
set noscrollbars
command! -nargs=* Google tabopen google <args>
command! -nargs=* Translate tabopen translate <args>
command! -nargs=* Wiki tabopen wikipedia <args>

" vim: set ft=vimperator:
noremap m :tabprevious<CR>
noremap , :tabnext<CR>
